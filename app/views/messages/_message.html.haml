%div{class:'cont-' + message.user.id.to_s}
    %div{class:'message-box msg-' + message.user.id.to_s}
        - unless message.channel.group == false
            %h6.name
                = complete_name(message.user)
            %p.time
                = "#{time_ago_in_words(message.created_at)} ago"
        = message.body